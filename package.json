{
  "name": "@matanlurey/tts-save-files",
  "version": "0.1.0",
  "description": "Interfaces for Tabletop Simulator's save file format",
  "scripts": {
    "fix": "prettier --write . --ignore-path .gitignore && eslint --fix src/** --ext .json,.ts",
    "generate": "ts-node tool/generate.ts --output src/types.d.ts",
    "lint": "prettier --ignore-path .gitignore --list-different src && eslint src/** --ext .json,.ts",
    "test": "jest",
    "validate": "ajv -d \"test/saves/sample.json\" -s \"src/schema/SaveState.json\" -r \"src/schema/**/*.json\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matanlurey/tts-save-files.git"
  },
  "main": "src/index.d.ts",
  "author": "Matan Lurey",
  "keywords": [
    "Tabletop",
    "Simulator",
    "TTS"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^25.2.1",
    "@types/minimist": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "ajv-cli": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-prettier": "^3.1.3",
    "fs-extra": "^9.0.0",
    "jest": "^25.5.4",
    "json-schema-to-typescript": "^9.0.1",
    "minimist": "^1.2.5",
    "prettier": "^2.0.5",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  }
}
